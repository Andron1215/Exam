<Window x:Class="Cvety.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cvety"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="450" MinWidth="800"
		WindowStartupLocation="CenterScreen">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<RowDefinition/>
		</Grid.RowDefinitions>
		<StackPanel Orientation="Horizontal" Background="#FF76E383" Height="70">
			<Image Source="/Resources/logo.png" Height="50" Width="50" Margin="10 0 0 0"/>
		</StackPanel>
		<Frame x:Name="frmMain" Grid.Row="1" NavigationUIVisibility="Hidden" Margin="10"/>
	</Grid>
</Window>
